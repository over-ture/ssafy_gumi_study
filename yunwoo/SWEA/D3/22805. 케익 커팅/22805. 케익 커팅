import sys
sys.stdin = open("test.txt")
num = int(input())
for seq in range(1,num+1):
    n = int(input())
    arr = [list(map(int,input().split())) for _ in range(n)]
    result = 0

    for i in range(1,n):
        for j in range(1,n):
            a = 0
            b = 0
            c = 0
            d = 0

            for row in arr[0:i]:
                a += sum(row[0:j])
                b += sum(row[j:n])

            for row in arr[i:n]:
                c += sum(row[0:j])
                d += sum(row[j:n])

            if a==b and b==c and c==d :
                result = 1
                break
    print(f'{seq} {result}')